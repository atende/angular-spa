{
  "name": "angular-spa",
  "version": "0.3.4",
  "description": "Utilities for angular 2 applications backed by generator-spring-spa project",
  "main": "index.js",
  "angular-cli": {},
  "scripts": {
    "lint": "tslint ./src/**/*.ts -t verbose",
    "lint:fix": "tslint ./src/**/*.ts --fix",
    "test": "ng test",
    "test-once": "ng test --watch=false",
    "compile": "cd src && tsc",
    "watch": "cd src && tsc --watch",
    "build": "npm run compile",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "webdriver:update": "webdriver-manager update",
    "copyfiles": "cp -rf package.json README.md LICENSE dist/out-tsc/app",
    "clean": "rm -rf dist",
    "dist": "npm run clean && npm run build && npm run copyfiles && cd dist/out-tsc/app && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atende/angular-spa.git"
  },
  "keywords": [
    "angular2",
    "ng2",
    "utils",
    "ui",
    "keycloak",
    "security"
  ],
  "author": "Giovanni Silva",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/atende/angular-spa/issues"
  },
  "homepage": "https://github.com/atende/angular-spa#readme",
  "dependencies": {
    "@angular/common": ">=2.0.0 <=3.0.0",
    "@angular/core": ">=2.0.0 <=3.0.0",
    "@angular/http": ">=2.0.0 <=3.0.0",
    "@angular/router": ">=3.0.0 <=4.0.0",
    "@angular/platform-browser": ">=2.0.0 <=3.0.0",
    "angular-http-interceptor": "^0.2.3",
    "rxjs": ">=5.0.0-beta.12 <= 5.0.0-rc.4",
    "core-js": "^2.4.1",
    "zone.js": ">=0.6.26 < 2.0.0",
    "keycloak-js": "^2.4.0"
  },
  "devDependencies": {
    "@angular/compiler": ">=2.0.0 <=3.0.0",
    "@angular/platform-browser-dynamic": ">=2.0.0 <=3.0.0",
    "@types/jasmine": "^2.2.30",
    "@types/node": "^6.0.42",
    "angular-cli": "1.0.0-beta.19-3",
    "codelyzer": "1.0.0-beta.1",
    "jasmine-core": "2.4.1",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-spec-reporter": "0.0.26",
    "protractor": "4.0.9",
    "ts-node": "1.2.1",
    "tslint": "3.13.0",
    "typescript": "~2.0.3",
    "webdriver-manager": "10.2.5"
  }
}
